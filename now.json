{
  "version": 2,
  "name": "minelev-logs",
  "scope": "minelev",
  "alias": [
    "main.logs.minelev.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_CONNECTION": "@db-cosmos-prod-connection",
    "MONGODB_COLLECTION": "@db-cosmos-prod-collection-logs-name",
    "MONGODB_NAME": "@db-cosmos-prod-db-name",
    "JWT_SECRET": "@logs-service-jwt-secret",
    "PAPERTRAIL_HOSTNAME": "minelev",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port"
  },
  "builds": [
    {
      "src": "/index.js",
      "use": "@now/node"
    },
    {
      "src": "/lib/ping.js",
      "use": "@now/node"
    },
    {
      "src": "/README.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    {
      "src": "/logs",
      "dest": "/index.js"
    },
    {
      "src": "/logs/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/queue",
      "dest": "/index.js"
    },
    {
      "src": "/queue/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/classes/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/ping",
      "dest": "/lib/ping.js"
    },
    { "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/README.html"
    }
  ]
}